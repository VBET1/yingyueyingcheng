   <span slot=\"label\">\r\n            <el-tooltip content=\"访问的组件路径，如：`system/user/index`，默认在`views`目录下\" placement=\"top\">\r\n              <i class=\"el-icon-question\"></i>\r\n            </el-tooltip>\r\n            组件路径\r\n          </span>\r\n          <el-input v-model=\"sysMenu.component\" placeholder=\"请输入组件路径\" />\r\n        </el-form-item>\r\n        <el-form-item v-if=\"sysMenu.type === 2\">\r\n          <el-input v-model=\"sysMenu.perms\" placeholder=\"请输入权限标识\" maxlength=\"100\" />\r\n          <span slot=\"label\">\r\n            <el-tooltip content=\"控制器中定义的权限字符，如：@PreAuthorize(hasAuthority('bnt.sysRole.list'))\" placement=\"top\">\r\n              <i class=\"el-icon-question\"></i>\r\n            </el-tooltip>\r\n            权限字符\r\n          </span>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\" prop=\"type\">\r\n          <el-radio-group v-model=\"sysMenu.status\">\r\n            <el-radio :label=\"1\">正常</el-radio>\r\n            <el-radio :label=\"0\">停用</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button size=\"small\" icon=\"el-icon-refresh-right\" @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-check\" size=\"small\" @click=\"saveOrUpdate()\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from '@/api/menu/menu.js'\r\nconst defaultForm = {\r\n  id: '',\r\n  parentId: '',\r\n  name: '',\r\n  type: 0,\r\n  path: '',\r\n  component: '',\r\n  perms: '',\r\n  icon: '',\r\n  sortValue: 1,\r\n  status: 1\r\n}\r\nexport default {\r\n  // 定义数据\r\n  data() {\r\n    return {\r\n      sysMenuList: [],\r\n      expandKeys: [], // 需要自动展开的项\r\n\r\n      typeDisabled: false,\r\n      type0Disabled: false,\r\n      type1Disabled: false,\r\n      type2Disabled: false,\r\n      dialogTitle: '',\r\n\r\n      dialo